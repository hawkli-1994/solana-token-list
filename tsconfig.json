{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "outDir": "lib",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2021",
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@utils/*": ["src/utils/*"],
      "@moneygram/*": ["src/moneygram/*"],
      "@moneygramSEP24/*": ["src/moneygram/moneygramSEP24"],
      "@supportfunctions/*": ["src/supportfunctions/*"],
      "@stellarTypes/*": ["src/types/*"]
    }
  },
  "compileOnSave": true,
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "lib"]
}
